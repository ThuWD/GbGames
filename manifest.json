{
    "name": "GameBoy Games",
    "description": "All your favourite GameBoy Games at your fingertips!",
    "manifest_version": 2,
    "version": "0.0.2",
    "icons": {
        "128": "icons/icon_128.png",
        "96": "icons/icon_96.png",
        "80": "icons/icon_80.png",
